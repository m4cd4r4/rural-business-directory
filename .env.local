# Development environment variables
# Copy this file to .env.local and update with your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_SUBMISSIONS=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_CACHING=true

# External Services (Development)
REACT_APP_GOOGLE_MAPS_API_KEY=your_development_maps_key
REACT_APP_ANALYTICS_ID=

# Debug Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# Strapi CMS (if using)
REACT_APP_STRAPI_URL=http://localhost:1337
REACT_APP_STRAPI_API_TOKEN=your_strapi_token

# Contentful CMS (if using)
REACT_APP_CONTENTFUL_SPACE_ID=your_contentful_space_id
REACT_APP_CONTENTFUL_ACCESS_TOKEN=your_contentful_access_token
REACT_APP_CONTENTFUL_ENVIRONMENT=master
REACT_APP_CONTENTFUL_MANAGEMENT_TOKEN=your_contentful_management_token

# Firebase (if using)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Supabase (if using)
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Australian Business Register (if using)
REACT_APP_ABR_GUID=your_abr_guid
